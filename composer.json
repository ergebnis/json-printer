{
  "name": "ergebnis/json-printer",
  "type": "library",
  "description": "Provides a JSON printer, allowing for flexible indentation.",
  "keywords": [
    "json",
    "printer",
    "formatter"
  ],
  "homepage": "https://github.com/ergebnis/json-printer",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "ergebnis/php-cs-fixer-config": "~1.1.0",
    "ergebnis/phpstan-rules": "~0.14.1",
    "ergebnis/test-util": "~0.9.0",
    "infection/infection": "~0.13.6",
    "phpbench/phpbench": "~0.16.10",
    "phpstan/extension-installer": "^1.0.3",
    "phpstan/phpstan": "~0.11.19",
    "phpstan/phpstan-deprecation-rules": "~0.11.2",
    "phpstan/phpstan-strict-rules": "~0.11.1",
    "phpunit/phpunit": "^8.5.0"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Json\\Printer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Json\\Printer\\Test\\": "test/"
    }
  },
  "support": {
    "issues": "https://github.com/ergebnis/json-printer/issues",
    "source": "https://github.com/ergebnis/json-printer"
  }
}
